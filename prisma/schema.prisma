generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model Character {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  bio     String
  height  String
  like    String
  name    String
  notlike String
  weight  String
  vitality Int?
  moves    Move[]
}
model Move {
  id                  String @id @default(auto()) @map("_id") @db.ObjectId
  character           Character @relation(fields: [characterId], references: [id])
  characterId         String @db.ObjectId
  name                String
  type                String?
  driveGauge          Int?  
  video               String?
  definition          String?
  classic             String
  modern              String?
  manual              String?
  startup             Int?
  active              String?
  missRecovery        Int?
  hitStunRecovery     Int?
  blockStunRecovery   Int?
  cancelable          String?
  damage              Int?
  driveIncreaseHit    Int?
  driveDecreaseBlock  Int?
  driveDecreasePunish Int?
  superArtGaugeIncrease Int?
  properties          String?
  scaling             String? 
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
  name  String
}

model data0 {
  id  String @id @default(auto()) @map("_id") @db.ObjectId
  msg String
}
